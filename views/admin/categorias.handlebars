<h2>Lista de categorias</h2>
<hr>
<a><button class="btnbtn-success">Nova categoria</button></a>

{{#each categorias}}
    <div class="card">
        <div class="card-body">
            <h4>{{nome}}</h4>
            <small>Slug {{slug}}</small><br>
            <small>Data de criação: {{date}}</small>
        </div>
    </div>
{{else}}

{{/each}}